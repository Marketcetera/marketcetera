<%= render :partial => 'shared/breadcrumbs', 
    :locals=> { :top_level => "Trades", :action_name => "Update Trade", :top_level_controller => 'trades' } %>
<h1>Update Trade</h1>

<%= start_form_tag :action => 'update', :id => @trade %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Update', :class => "submit floatright" %>
<%= end_form_tag %>

<%= button_to 'Cancel', {:action => 'show', :id => @trade}, {:class => "reset floatright"} %>

<%= render :partial => 'shared/breadcrumbs', 
    :locals=> { :top_level => "Symbols", :action_name => "Update Symbol", :top_level_controller => @controller.controller_name } %>
<h1>Update Symbol</h1>

<% form_tag :action => 'update', :id => @m_symbol do -%>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Update', :class => "submit floatright" %>
<% end -%>

<%= button_to 'Cancel', {:action => 'show', :id => @m_symbol}, {:class => "reset floatright"} %>

<%= render :partial => 'shared/breadcrumbs', 
    :locals=> { :top_level => "Dividends", :action_name => "Edit Dividend", :top_level_controller => @controller } %>
<h1>Edit Dividend</h1>

<%= start_form_tag :action => 'update', :id => @dividend %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Update', :class => "submit floatright" %>
<%= end_form_tag %>
<%= button_to 'Cancel', {:action => 'show', :id => @dividend}, {:class => "reset floatright"} %>

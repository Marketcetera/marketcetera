<%= render :partial => 'shared/breadcrumbs', 
    :locals=> { :top_level => "Equities", :action_name => "Update Equity", :top_level_controller => @controller } %>
<h1>Editing equity</h1>

<%= start_form_tag :action => 'update', :id => @equity %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Update', :class => "submit floatright" %>
<%= end_form_tag %>

<%= button_to 'Cancel', {:action => 'show', :id => @equity}, {:class => "reset floatright"} %>

<?xml version="1.0" encoding="UTF-8"?>

<!-- START SNIPPET: spring -->
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">
<beans>

  <!-- Spring JMS Template -->
  <bean id="quoteJmsTemplate" class="org.springframework.jms.core.JmsTemplate">
    <property name="connectionFactory">
      <ref parent="internalConnectionFactory"/>
    </property>
	<property name="defaultDestination" ref="quotesTopic"/>
  </bean>

  <bean id="quoteFeed" class="org.marketcetera.bogusfeed.BogusFeed">
  	<property name="quoteJmsTemplate" ref="quoteJmsTemplate"/>
  </bean>

</beans>  
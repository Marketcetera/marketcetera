<beans
 xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation=
 "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <import
   resource="qdescriptor.xml"/>
  <import
   resource="modifiers.xml"/>
  <import
   resource="routes.xml"/>

  <bean
   id="metc_broker1_broker"
   class="org.marketcetera.ors.brokers.SpringBroker">
    <property
     name="name"
     value="Marketcetera Exchange 1"/>
    <property
     name="id"
     value="metc1"/>
    <property
     name="descriptor"
     ref="metc_broker1_qdescriptor"/>
    <property
     name="modifiers"
     ref="metc_broker1_modifiers"/>
    <property
     name="routes"
     ref="metc_broker1_routes"/>
  </bean>
</beans>

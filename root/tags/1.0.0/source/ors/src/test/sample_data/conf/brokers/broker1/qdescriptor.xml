  - Author: tlerios@marketcetera.com
  - Since: 1.0.0
  - Version: $Id$
  - $License$
  -->

<beans
 xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation=
 "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean
   id="metc_broker1_qdescriptor"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="${metc.qf.broker1.begin.string}"/>
        <entry
         key="DataDictionary"
         value="${metc.qf.broker1.data.dictionary}"/>
        <entry
         key="SenderCompID"
         value="${metc.qf.broker1.sender.comp.id}"/>
        <entry
         key="TargetCompID"
         value="${metc.qf.broker1.target.comp.id}"/>
        <entry
         key="FileStorePath"
         value="${metc.qf.broker1.store}"/>
        <entry
         key="SocketConnectHost"
         value="${metc.qf.broker1.host}"/>
        <entry
         key="SocketConnectPort"
         value="${metc.qf.broker1.port}"/>
      </map>
    </property>
  </bean>
</beans>

<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:jpa="http://aries.apache.org/xmlns/jpa/v1.0.0"
           xmlns:tx="http://aries.apache.org/xmlns/transactions/v1.1.0"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
    <service ref="bogusFeedBean" interface="org.marketcetera.marketdata.provider.MarketDataProvider"/>
    <bean id="bogusFeedBean" class="org.marketcetera.marketdata.bogus.BogusFeed" init-method="start">
        <property name="providerRegistry" ref="marketDataProviderRegistry"/>
        <property name="symbolResolver" ref="symbolResolverService"/>
    </bean>
    <reference id="marketDataProviderRegistry" interface="org.marketcetera.marketdata.manager.MarketDataProviderRegistry"/>
    <reference id="symbolResolverService" interface="org.marketcetera.core.symbolresolver.SymbolResolver"/>
</blueprint>

<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://www.osgi.org/xmlns/blueprint/v1.0.0 http://www.osgi.org/xmlns/blueprint/v1.0.0/blueprint.xsd">
  <bean id="futureSymbolResolverBean" class="org.marketcetera.symbolresolver.FutureSymbolResolver"/>
  <bean id="osiOptionSymbolResolverBean" class="org.marketcetera.symbolresolver.FutureSymbolResolver"/>
  <bean id="equitySymbolResolverBean" class="org.marketcetera.symbolresolver.EquitySymbolResolver"/>
  <bean id="convertibleBondSymbolResolverBean" class="org.marketcetera.symbolresolver.ConvertibleBondSymbolResolver"/>
  <bean id="marketceteraSymbolResolverBean" class="org.marketcetera.symbolresolver.MarketceteraSymbolResolverManager">
    <property name="resolvers">
      <list>
        <ref component-id="futureSymbolResolverBean"/>
        <ref component-id="osiOptionSymbolResolverBean"/>
        <ref component-id="convertibleBondSymbolResolverBean"/>
        <ref component-id="equitySymbolResolverBean"/>
      </list>
    </property>
  </bean>
  <service interface="org.marketcetera.core.symbolresolver.SymbolResolver" ref="marketceteraSymbolResolverBean" />
</blueprint>

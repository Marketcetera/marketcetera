<beans
 xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation=
 "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

  <bean
   id="goodDescriptor"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="BeginStringValue"/>
        <entry
         key="SenderCompID"
         value="SenderCompIDValue"/>
        <entry
         key="SenderSubID"
         value="SenderSubIDValue"/>
        <entry
         key="SenderLocationID"
         value="SenderLocationIDValue"/>
        <entry
         key="TargetCompID"
         value="TargetCompIDValue"/>
        <entry
         key="TargetSubID"
         value="TargetSubIDValue"/>
        <entry
         key="TargetLocationID"
         value="TargetLocationIDValue"/>
        <entry
         key="SessionQualifier"
         value="SessionQualifierValue"/>
        <entry
         key="DataDictionary"
         value="DataDictionaryValue"/>
        <entry
         key="testKey"
         value="testKeyValue"/>
      </map>
    </property>
  </bean>

  <bean
   id="missingBeginString"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor"/>

  <bean
   id="missingSenderCompId"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="settings">
      <bean
       class="org.marketcetera.util.quickfix.SpringSessionSettings"/>
    </property>
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="BeginStringValue"/>
        <entry
         key="TargetCompID"
         value="TargetCompIDValue"/>
      </map>
    </property>
  </bean>

  <bean
   id="missingTargetCompId"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="settings">
      <bean
       class="org.marketcetera.util.quickfix.SpringSessionSettings">
        <property
         name="defaults">
          <map>
            <entry
             key="TargetCompID"
             value="TargetCompIDValue"/>
          </map> 
        </property>
      </bean>
    </property>
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="BeginStringValue"/>
        <entry
         key="SenderCompID"
         value="SenderCompIDValue"/>
        <entry
         key="TargetCompID">
          <null/>
        </entry>
      </map>
    </property>
  </bean>

  <bean
   id="goodDefaultDictionary"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="settings">
      <bean
       class="org.marketcetera.util.quickfix.SpringSessionSettings">
        <property
         name="defaults">
          <map>
            <entry
             key="BeginString"
             value="FIX.4.1"/>
            <entry
             key="SenderCompID"
             value="SenderCompIDValue"/>
            <entry
             key="TargetCompID"
             value="TargetCompIDValue"/>
            <entry
             key="DataDictionary"
             value="metc.DefaultDataDictionary"/>
          </map> 
        </property>
      </bean>
    </property>
  </bean>

  <bean
   id="badDefaultDictionary"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="FIX.3.0"/>
        <entry
         key="SenderCompID"
         value="SenderCompIDValue"/>
        <entry
         key="TargetCompID"
         value="TargetCompIDValue"/>
        <entry
         key="DataDictionary"
         value="metc.DefaultDataDictionary"/>
      </map>
    </property>
  </bean>

  <bean
   id="inheritsDefaults"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="settings">
      <bean
       class="org.marketcetera.util.quickfix.SpringSessionSettings">
        <property
         name="defaults">
          <map>
            <entry
             key="BeginString"
             value="FIX.4.1"/>
            <entry
             key="SenderCompID"
             value="SenderCompIDValue"/>
            <entry
             key="TargetCompID"
             value="TargetCompIDValue"/>
          </map> 
        </property>
      </bean>
    </property>
    <property
     name="dictionary">
      <map>
        <entry
         key="TargetCompID"
         value="TargetCompIDValueOverride"/>
        <entry
         key="DataDictionary"
         value="metc.DefaultDataDictionary"/>
      </map>
    </property>
  </bean>
</beans>

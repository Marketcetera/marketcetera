  -->

<beans
 xmlns="http://www.springframework.org/schema/beans"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation=
 "http://www.springframework.org/schema/beans
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

  <!--
    - See
    - http://www.quickfixj.org/quickfixj/usermanual/usage/configuration.html
    - for a complete list of standard options.
    -->

  <bean
   id="metc_qdescriptor"
   class="org.marketcetera.util.quickfix.SpringSessionDescriptor">
    <property
     name="dictionary">
      <map>
        <entry
         key="BeginString"
         value="FIX.4.2"/>
        <!--
          - An acceptable value for this option is
          - 'metc.DefaultDataDictionary', which will choose the standard
          - QuickFIX/J dictionary for the FIX version deduced from the
          - 'BeginString' option.
          -->
        <entry
         key="DataDictionary"
         value="metc.DefaultDataDictionary"/>
        <entry
         key="SenderCompID"
         value="enduser@enduserdomain.com"/>
        <entry
         key="TargetCompID"
         value="MRKTC-EXCH"/>
        <entry
         key="FileStorePath"
         value="${org.marketcetera.appDir}/ors/broker/qf_store"/>
        <entry
         key="SocketConnectHost"
         value="exchange.marketcetera.com"/>
        <entry
         key="SocketConnectPort"
         value="7001"/>
      </map>
    </property>
  </bean>
</beans>
